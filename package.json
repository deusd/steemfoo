{
  "name": "vybe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pretest": "npm run lint",
    "test": "jest",
    "test:only": "jest",
    "test:watch": "jest --watch",
    "ios": "react-native run-ios --scheme Production",
    "android-prod": "ENVFILE=.env.prod react-native run-android",
    "build-android-prod": "export ENVFILE=.env.prod && cd android && ./gradlew assembleRelease && cd ..",
    "rnlink": "react-native link",
    "upgrade-rn": "react-native-git-upgrade"
  },
  "dependencies": {
    "apollo-boost": "0.1.10",
    "axios": "0.18.0",
    "firebase": "5.0.4",
    "flux-standard-action": "2.0.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "native-base": "2.7.1",
    "numeral": "2.0.6",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-apollo": "2.1.9",
    "react-native": "0.55.4",
    "react-native-actionsheet": "2.4.2",
    "react-native-app-auth": "2.4.1",
    "react-native-config": "0.11.5",
    "react-native-fetch-blob": "0.10.8",
    "react-native-image-picker": "0.26.10",
