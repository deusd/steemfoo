    "sinon": "5.0.10"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      ".+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/samples/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native|react-navigation)"
    ],
    "setupTestFrameworkScriptFile": "./jest/setup.js"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
